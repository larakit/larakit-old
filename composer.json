{
    "name": "larakit/larakit",
    "description": "The Larakit",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "hirak/prestissimo": "^0.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-quickform.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/sf-m-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-auth-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-auth.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/project.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-page.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-widget.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-telegrambot.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-thumb.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-hashids.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-verstak.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:larakit/lk-route.git"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
